    <!-- Tractor Transporter -->
      <Group>
        <!-- New Transporter -->
          <ModOp Type="addNextSibling" GUID="269770" Condition="!@1500300031">
            <Asset>
              <Template>TrafficFeedbackUnit</Template>
              <Values>
                <Standard>
                  <GUID>1500300031</GUID>
                  <Name>Tractor (Generic)</Name>
                  <IconFilename>data/ui/2kimages/main/3dicons/icon_mineral_desposits.png</IconFilename>
                </Standard>
                <TrafficFeedbackUnit>
                  <UnitType>Vehicle</UnitType>
                  <CfgPath>data/dlc05/graphics/vehicle/agricultural_vehicle/agriculture_vehicle_01_front.cfg</CfgPath>
                  <SpeedSource>TakeFromAsset</SpeedSource>
                  <OverwriteSpeed>0.25</OverwriteSpeed>
                  <ResidentCamInteraction>MountAsMotorized</ResidentCamInteraction>
                </TrafficFeedbackUnit>
                <Trailer>
                  <TrailerCount>1</TrailerCount>
                  <TrailerEndPartCfgVariations>
                    <Item>
                      <CfgVariation>data/dlc05/graphics/vehicle/agricultural_vehicle/trailer_01_transport_back.cfg</CfgVariation>
                    </Item>
                  </TrailerEndPartCfgVariations>
                </Trailer>
              </Values>
            </Asset>
            <Asset>
              <Template>FeedbackUnitClass</Template>
              <Values>
                <Standard>
                  <GUID>1500300039</GUID>
                  <Name>Tractor (Generic)</Name>
                  <IconFilename>data/ui/2kimages/main/3dicons/icon_mineral_desposits.png</IconFilename>
                </Standard>
                <TrafficFeedbackClass>
                  <DefaultBehaviour>FarmWorkerMotorized</DefaultBehaviour>
                  <UnitAssets>
                    <Item>
                      <UnitAsset>1500300031</UnitAsset>
                    </Item>
                  </UnitAssets>
                  <AllowedReactions />
                  <UseSpecialPathPlannerLayer_Deprecated>1</UseSpecialPathPlannerLayer_Deprecated>
                  <SpecialPathPlannerLayer>FarmFields_Vehicle</SpecialPathPlannerLayer>
                </TrafficFeedbackClass>
              </Values>
            </Asset>
          </ModOp>
        <!-- Transporter Unit which uses the the tractor plus 1 trailer is replacing the standard Residence Transporter -->
          <ModOp Type="merge" GUID="2001096" Path="/Values/FeedbackSessionDescription/LogisticConfig/TransporterClasses">
            <ResidenceTransporter>
              <TransporterClass>1500300039</TransporterClass>
            </ResidenceTransporter>
          </ModOp>
          <ModOp Type="merge" GUID="141486" Path="/Values/FeedbackSessionDescription/LogisticConfig/TransporterClasses">
            <ResidenceTransporter>
              <TransporterClass>269966</TransporterClass>
            </ResidenceTransporter>
          </ModOp>
          <ModOp Type="merge" GUID="142872" Path="/Values/FeedbackSessionDescription/LogisticConfig/TransporterClasses">
            <ResidenceTransporter>
              <TransporterClass>123977</TransporterClass>
            </ResidenceTransporter>
          </ModOp>
        <!-- New transporter unit moves at a slightly slower speed as electrified carriages and unloads 5 times faster than the regular transporter unit -->
          <ModOp Type="merge" GUID="1010106" Path="Values/Transporter7">
            <ProcessingTimes>
              <LoadingTime>4</LoadingTime>
              <UnloadingTime>4</UnloadingTime>
            </ProcessingTimes>
            <TransporterSpeed>1</TransporterSpeed>
          </ModOp>
          <ModOp Type="remove" GUID="269969" Path="/Values/TrafficFeedbackUnit/OverwriteSpeed" AllowNoMatch="1"></ModOp>
      </Group>
