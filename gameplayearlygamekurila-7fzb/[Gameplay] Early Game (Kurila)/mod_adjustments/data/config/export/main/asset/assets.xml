      <ModOp Type="replace" GUID="15000002" Path="/Values/PopulationLevel7/PopulationInputs/Item[Product='1010198']/Amount" Condition="/Values/PopulationLevel7/PopulationInputs/Item[Product='1010198']">
          <Amount>0.0008333336</Amount>
      </ModOp>
    </Group>
