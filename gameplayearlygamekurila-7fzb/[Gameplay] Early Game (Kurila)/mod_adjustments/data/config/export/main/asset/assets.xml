      <!--  Replace Fish Chain with Basic Food Category -->
        <ModOp Type="replace" GUID="25000189,500945" Path="/Values/ConstructionCategory/BuildingList/Item[Building='1500010846']/Building"
          Condition="/Values/ConstructionCategory/BuildingList/Item[Building='1500010846']">
            <Building>1500301176</Building>
        </ModOp>
        <ModOp GUID='15000000' Type="addNextSibling">
          <Asset>
            <Template>Icon</Template>
            <Values>
              <Standard>
                <GUID>1500301177</GUID>
                <Name>Vegetables Alt Icon</Name>
                <IconFilename>data/ui/2kimages/main/kurila/icon_vegetables_2.png</IconFilename>
              </Standard>
            </Values>
          </Asset>
        </ModOp>
        <ModOp Type="merge" GUID="100654" Path="/Values" Condition="@25000244/ProductionChain/Tier1/Item[ForceIcon='1500010952']">
          <Constructable>
            <ConstructionMenuIcon>1500301177</ConstructionMenuIcon>
          </Constructable>
        </ModOp>
    </Group>
  <!-- Basic Food Integration: If Fish Farm is activ  -->
    <Group Condition="@2084100600">
      <!--  Replace Fish Chain with Basic Food Category -->
        <ModOp Type="replace" GUID="25000189" Path="/Values/ConstructionCategory/BuildingList/Item[Building='2084100600']/Building"
          Condition="/Values/ConstructionCategory/BuildingList[Item/Building='2084100600'and not(Item/Building='1500301176')]">
            <Building>1500301176</Building>
        </ModOp>
        <ModOp Type="add" GUID="1500301176" Path="/Values/ConstructionCategory/BuildingList">
          <Item>
            <Building>2084100601</Building>
          </Item>
        </ModOp>
    </Group>
  <!-- Basic Food Integration: If no mod is activ  -->
    <Group>
      <ModOp Type="replace" GUID="25000189,500945" Path="/Values/ConstructionCategory/BuildingList/Item[Building='1010278']"
        Condition="/Values/ConstructionCategory/BuildingList/Item[Building='1010278']">
        <Item>
          <Building>1500301176</Building>
        </Item>
      </ModOp>
    </Group>
  <!-- Adjust Red Pepper consume if Goulash Works reduce Cycletime from Red Pepper Farm -->
