        Condition="/Values/ConstructionCategory/BuildingList/Item[Building='1999002381']">
        <Item>
          <Building>1500301176</Building>
        </Item>
      </ModOp>
      <ModOp Type="addNextSibling" GUID="1500301176" Path="/Values/ConstructionCategory/BuildingList/Item[Building='1010278']" Condition="/Values/ConstructionCategory/BuildingList/Item[Building='1010278']">
        <Item>
          <Building>1999002380</Building>
        </Item>
      </ModOp>
    </Group>
  <!-- Basic Food Integration: If Jakobs mod ist activ  -->
