	<!-- PURPOSE: Description for Wild Custard Apple item -->
	<ModOp Path='/TextExport/Texts' Type="Add">
		<Text>
			<GUID>1660011121</GUID>
			<Text>Annona senegalensis. "Prima sono venuti quei pini tropicali e ora un altro? Un sapore così strano, ma non sgradevole".</Text>
		</Text>
	</ModOp>

	<!-- GROUP: Botanical set: Bountiful Branches -->
	<!-- PURPOSE: Title for Ironwood Olive Tree item -->
	<ModOp Path='/TextExport/Texts' Type="Add">
		<Text>
			<GUID>1660011210</GUID>
			<Text>Legno d'ulivo di ferro</Text>
