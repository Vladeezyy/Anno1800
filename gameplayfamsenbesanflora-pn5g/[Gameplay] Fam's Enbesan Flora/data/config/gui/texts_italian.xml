	<!-- PURPOSE: Description for Ironwood Olive Tree item -->
	<ModOp Path='/TextExport/Texts' Type="Add">
		<Text>
			<GUID>1660011211</GUID>
			<Text>Olea capensis. Uno dei legni più duri al mondo, il legno duro è stato usato per secoli per arte e manufatti decorativi, con una regolare raccolta di olive.</Text>
		</Text>
	</ModOp>

	<!-- GROUP: Botanical set: Bountiful Branches -->
	<!-- PURPOSE: Title for Moringa Tree item -->
	<ModOp Path='/TextExport/Texts' Type="Add">
		<Text>
			<GUID>1660011212</GUID>
			<Text>Albero della Moringa</Text>
		</Text>
	</ModOp>

