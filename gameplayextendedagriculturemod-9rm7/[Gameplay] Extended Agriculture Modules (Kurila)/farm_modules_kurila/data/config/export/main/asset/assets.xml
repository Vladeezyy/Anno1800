            <ModOp Type="remove" GUID="1956,1961" Path="/Values/FactoryUpgrade/ReplaceInputs"></ModOp>
            <ModOp Type="remove" GUID="1956,1961" Path="/Values/BuildingUpgrade" Condition="!@25240/Maintenance/Maintenances"></ModOp>
            <ModOp Type="add" GUID="193967,192838,193057" Path="/Values/RewardPool/ItemsPool">
              <Item>
                <ItemLink>1956</ItemLink>
              </Item>
              <Item>
                <ItemLink>1961</ItemLink>
