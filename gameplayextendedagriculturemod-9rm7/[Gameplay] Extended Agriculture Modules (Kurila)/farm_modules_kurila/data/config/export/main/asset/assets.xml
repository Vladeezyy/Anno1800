  <!-- Imya Tweaks -->
    <ImyaTweaks Title="Features of Farm Modules" Icon="data/ui/2kimages/main/3dicons/icon_tractor.png" />
    <ImyaExpose Path="*" Kind="SkipToggle" InitState="True" ModOpID="FarmModules" ExposeID="Disable/Enable Farm Tractors">
    </ImyaExpose>
    <ImyaExpose Path="*" Kind="SkipToggle" InitState="True" ModOpID="ModuleMaintenance" ExposeID="Disable/Enable Maintenance for Modules">
    </ImyaExpose>
    <ImyaExpose Path="*" Kind="SkipToggle" InitState="True" ModOpID="ModuleAttractiveness" ExposeID="Disable/Enable Negative Attractiveness for Fertiliser Silos">
    </ImyaExpose>
