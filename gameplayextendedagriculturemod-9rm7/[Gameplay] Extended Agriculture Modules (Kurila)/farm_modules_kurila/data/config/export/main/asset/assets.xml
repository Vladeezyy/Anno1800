    <!-- Adjust Items -->
      <Group>
        <!-- Orchard Pedologists -->
          <Group>
            <ModOp Type="merge" GUID="1956,1961" Path="/Values">
              <ItemEffect>
                <EffectTargets>
                  <Item>
                    <GUID>1500301025</GUID>
