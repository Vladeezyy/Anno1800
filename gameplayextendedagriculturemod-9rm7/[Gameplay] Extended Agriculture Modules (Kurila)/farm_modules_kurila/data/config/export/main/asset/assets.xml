        <!-- Fertiliser Silos have Negative Attractiveness -->
          <Group ModOpID="ModuleAttractiveness">
            <ModOp Type="merge" GUID='25240,25241,25242' Path="/Values">
              <Culture>
                <Attractiveness>-5</Attractiveness>
                <CultureType>Military</CultureType>
