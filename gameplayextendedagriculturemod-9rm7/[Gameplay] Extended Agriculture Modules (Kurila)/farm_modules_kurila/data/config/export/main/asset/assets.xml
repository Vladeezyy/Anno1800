<ModOps>
  <Group ModOpID="FarmModules">
    <!-- Adjust Farms -->
      <Group>
        <!-- Electricity for Farms-->
          <Group>
            <ModOp Type="merge" Path="//Values[ModuleOwner[FarmType='PlantFarm']]">
              <Industrializable>
                <BoostedByIndustrialization>1</BoostedByIndustrialization>
                <TransporterIndustrializationFactory>1010106</TransporterIndustrializationFactory>
                <TransporterIndustrializationWarehouse>1010106</TransporterIndustrializationWarehouse>
              </Industrializable>
              <Electrifiable>
                <ElectricityBoost>0</ElectricityBoost>
                <ShowPowerInObjectMenu>0</ShowPowerInObjectMenu>
              </Electrifiable>
              <FactoryBase>
                <ClampMaxTransporterLogisticCost>1</ClampMaxTransporterLogisticCost>
                <ClampedMaxTransporterLogisticCost>67</ClampedMaxTransporterLogisticCost>
              </FactoryBase>
            </ModOp>
          </Group>
        <!-- Electricity for Animal Farms-->
          <Group Condition="//Values[ModuleOwner/FarmType='AnimalFarm' and ModuleOwner/AdditionalModule='269837' or ModuleOwner/AdditionalModule='269958' or ModuleOwner/AdditionalModule='119025']">
              <ModOp Type="merge" Path="//Values[ModuleOwner/FarmType='AnimalFarm' and ModuleOwner/AdditionalModule='269837' or ModuleOwner/AdditionalModule='269958' or ModuleOwner/AdditionalModule='119025']">
                <Industrializable>
                  <BoostedByIndustrialization>1</BoostedByIndustrialization>
                  <TransporterIndustrializationFactory>1010106</TransporterIndustrializationFactory>
                  <TransporterIndustrializationWarehouse>1010106</TransporterIndustrializationWarehouse>
                </Industrializable>
                <Electrifiable>
                  <ElectricityBoost>0</ElectricityBoost>
                  <ShowPowerInObjectMenu>0</ShowPowerInObjectMenu>
                </Electrifiable>
                <FactoryBase>
                  <ClampMaxTransporterLogisticCost>1</ClampMaxTransporterLogisticCost>
                  <ClampedMaxTransporterLogisticCost>67</ClampedMaxTransporterLogisticCost>
                </FactoryBase>
              </ModOp>
              <ModOp Type="merge" GUID='1404423065' Path="/Values" Condition="@1404423065">
                <Industrializable>
                  <BoostedByIndustrialization>1</BoostedByIndustrialization>
                  <TransporterIndustrializationFactory>1010106</TransporterIndustrializationFactory>
                  <TransporterIndustrializationWarehouse>1010106</TransporterIndustrializationWarehouse>
                </Industrializable>
                <Electrifiable>
                  <ElectricityBoost>0</ElectricityBoost>
                  <ShowPowerInObjectMenu>0</ShowPowerInObjectMenu>
                </Electrifiable>
                <FactoryBase>
                  <ClampMaxTransporterLogisticCost>1</ClampMaxTransporterLogisticCost>
                  <ClampedMaxTransporterLogisticCost>67</ClampedMaxTransporterLogisticCost>
                </FactoryBase>
              </ModOp>
          </Group>
      </Group>
    <!-- Adjust Modules -->
      <Group>
        <!-- EffectContainer for Silos -->
          <ModOp Type="addNextSibling" GUID="25713">
            <Asset>
              <BaseAssetGUID>25713</BaseAssetGUID>
              <Values>
                <Standard>
                  <GUID>1500300918</GUID>
                  <Name>Fertiliser_Buff_2</Name>
                </Standard>
                <Text>
                  <TextOverride>25713</TextOverride>
                </Text>
                <FactoryUpgrade>
                  <AdditionalOutput>
                    <Item>
                      <ForceProductSameAsFactoryOutput>1</ForceProductSameAsFactoryOutput>
                      <AdditionalOutputCycle>2</AdditionalOutputCycle>
                      <Amount>3</Amount>
                    </Item>
                  </AdditionalOutput>
                </FactoryUpgrade>
              </Values>
            </Asset>
            <Asset>
              <BaseAssetGUID>25713</BaseAssetGUID>
              <Values>
                <Standard>
                  <GUID>1500300919</GUID>
                  <Name>Fertiliser_Buff_3</Name>
                </Standard>
                <Text>
                  <TextOverride>25713</TextOverride>
                </Text>
                <FactoryUpgrade>
                  <AdditionalOutput>
                    <Item>
                      <ForceProductSameAsFactoryOutput>1</ForceProductSameAsFactoryOutput>
                      <AdditionalOutputCycle>1</AdditionalOutputCycle>
                      <Amount>2</Amount>
                    </Item>
                  </AdditionalOutput>
                </FactoryUpgrade>
              </Values>
            </Asset>
            <Asset>
              <Template>EffectContainer</Template>
              <Values>
                <Standard>
                  <GUID>1500300909</GUID>
                  <Name>Fertiliser_Buff_Container_3</Name>
                  <IconFilename>data/ui/2kimages/main/3dicons/icon_silo.png</IconFilename>
                </Standard>
                <Text>
                  <TextOverride>25713</TextOverride>
                </Text>
                <EffectContainer>
                  <Effects>
                    <Item>
                      <Effect>1500300919</Effect>
                    </Item>
                  </Effects>
                </EffectContainer>
                <FactoryUpgrade>
                  <AdditionalOutput>
                    <Item>
                      <Product>1500300890</Product>
                      <AdditionalOutputCycle>1</AdditionalOutputCycle>
                      <Amount>1</Amount>
                    </Item>
                    <Item>
                      <Product>1500300891</Product>
                      <AdditionalOutputCycle>2</AdditionalOutputCycle>
                      <Amount>3</Amount>
                    </Item>
                    <Item>
                      <Product>1500300892</Product>
                      <AdditionalOutputCycle>1</AdditionalOutputCycle>
                      <Amount>2</Amount>
                    </Item>
                  </AdditionalOutput>
                </FactoryUpgrade>
              </Values>
            </Asset>
          </ModOp>
        <!-- Adjust Fertiliser Silos: Adding EffectContainer / Affected by Electricity / No Electricity Boost -->
          <Group>
            <ModOp Type="merge" GUID='25240,25241,25242' Path="/Values">
              <BuffFactory>
                <ProductionBuffs>
                  <Item>
                    <ProductivityBuff>25713</ProductivityBuff>
                    <ProductivityThresholdUp>0.01</ProductivityThresholdUp>
                  </Item>
                  <Item>
                    <ProductivityBuff>1500300918</ProductivityBuff>
                    <ProductivityThresholdUp>1.02</ProductivityThresholdUp>
                    <ProductivityThresholdDown>1.01</ProductivityThresholdDown>
                  </Item>
                  <Item>
                    <ProductivityBuff>1500300909</ProductivityBuff>
                    <ProductivityThresholdUp>1.52</ProductivityThresholdUp>
                    <ProductivityThresholdDown>1.51</ProductivityThresholdDown>
                  </Item>
                </ProductionBuffs>
              </BuffFactory>
              <Industrializable>
                <BoostedByIndustrialization>1</BoostedByIndustrialization>
                <TransporterIndustrializationFactory>1010106</TransporterIndustrializationFactory>
                <TransporterIndustrializationWarehouse>1010106</TransporterIndustrializationWarehouse>
              </Industrializable>
              <Electrifiable>
                <ElectricityBoost>0</ElectricityBoost>
                <ShowPowerInObjectMenu>0</ShowPowerInObjectMenu>
              </Electrifiable>
            </ModOp>
            <Group Condition="#item_sockets_kurila">
              <ModOp Type="merge" GUID='25240,25241,25242' Path="/Values">
                <Selection>
                  <UnlockSecondGUI>1500300092</UnlockSecondGUI>
                  <SecondGUIType>Guildhouse</SecondGUIType>
                  <AllowedDragType>ItemSocket;CargoItemStack;StorageItemStack;SharedTransferItem</AllowedDragType>
                </Selection>
                <ItemContainer>
                  <SocketCount>3</SocketCount>
                  <SocketAllocation>RadiusBuilding;Guildhouse;Mod1</SocketAllocation>
                  <SocketScope>Radius</SocketScope>
                </ItemContainer>
              </ModOp>
            </Group>
          </Group>
        <!-- Tractors, and Fertiliser Buffs give extra goods instead of productivity, reduced workforce is removed -->
          <Group>
            <!-- Tractors Buff -->
              <Group>
                <ModOp Type="remove" GUID='269979' Path="/Values/FactoryUpgrade/ProductivityUpgrade" Condition="/Values/FactoryUpgrade/ProductivityUpgrade"></ModOp>
                <ModOp Type="merge" GUID='269979' Path="/Values">
                  <Standard>
                    <InfoDescription>1500300943</InfoDescription>
                  </Standard>
                  <FactoryUpgrade>
                    <AdditionalOutput>
                      <Item>
                        <ForceProductSameAsFactoryOutput>1</ForceProductSameAsFactoryOutput>
                        <AdditionalOutputCycle>1</AdditionalOutputCycle>
                        <Amount>1</Amount>
                      </Item>
                    </AdditionalOutput>
                  </FactoryUpgrade>
                  <BuildingUpgrade>
                    <WorkforceAmountUpgrade>
                      <Value>-20</Value>
                      <Percental>1</Percental>
                    </WorkforceAmountUpgrade>
                  </BuildingUpgrade>
                </ModOp>
                <Group Condition="@1500300947">
                  <ModOp Type="remove" GUID='1500300945,1500300947' Path="/Values/FactoryUpgrade/ProductivityUpgrade"></ModOp>
                  <ModOp Type="merge" GUID='1500300945,1500300947' Path="/Values">
                    <FactoryUpgrade>
                      <AdditionalOutput>
                        <Item>
                          <ForceProductSameAsFactoryOutput>1</ForceProductSameAsFactoryOutput>
                          <AdditionalOutputCycle>1</AdditionalOutputCycle>
                          <Amount>1</Amount>
                        </Item>
                      </AdditionalOutput>
                    </FactoryUpgrade>
                    <BuildingUpgrade>
                      <WorkforceAmountUpgrade>
                        <Value>-20</Value>
                        <Percental>1</Percental>
                      </WorkforceAmountUpgrade>
                    </BuildingUpgrade>
                  </ModOp>
                </Group>
                <!-- Advanced Cattle Farm -->
                  <Group Condition="@1500300938">
                    <ModOp Type="remove" GUID='1500300938' Path="/Values/FactoryUpgrade/ProductivityUpgrade"></ModOp>
                    <ModOp Type="merge" GUID='1500300938' Path="/Values">
                      <Standard>
                        <InfoDescription>1500300943</InfoDescription>
                      </Standard>
                      <FactoryUpgrade>
                        <AdditionalOutput>
                          <Item>
                            <Product>1010193</Product>
                            <AdditionalOutputCycle>1</AdditionalOutputCycle>
                            <Amount>1</Amount>
                          </Item>
                          <Item>
                            <Product>5385</Product>
                            <AdditionalOutputCycle>1</AdditionalOutputCycle>
                            <Amount>1</Amount>
                          </Item>
                        </AdditionalOutput>
                      </FactoryUpgrade>
                      <BuildingUpgrade>
                        <WorkforceAmountUpgrade>
                          <Value>-20</Value>
                          <Percental>1</Percental>
                        </WorkforceAmountUpgrade>
                      </BuildingUpgrade>
                    </ModOp>
                    <ModOp Type="merge" GUID='1500300936' Path="/Values">
                      <Standard>
                        <InfoDescription>1500300943</InfoDescription>
                      </Standard>
                      <FactoryUpgrade>
                        <AdditionalOutput>
                          <Item>
                            <AdditionalOutputCycle>1</AdditionalOutputCycle>
                            <Amount>1</Amount>
                          </Item>
                        </AdditionalOutput>
                      </FactoryUpgrade>
                      <BuildingUpgrade>
                        <WorkforceAmountUpgrade>
                          <Value>-20</Value>
                          <Percental>1</Percental>
                        </WorkforceAmountUpgrade>
                      </BuildingUpgrade>
                    </ModOp>
                  </Group>
              </Group>
            <!-- Fertiliser Buff -->
              <Group>
                <ModOp Type="remove" GUID='25713' Path="/Values/FactoryUpgrade/ProductivityUpgrade" Condition="/Values/FactoryUpgrade/ProductivityUpgrade"></ModOp>
                <ModOp Type="merge" GUID='25713' Path="/Values/FactoryUpgrade">
                  <AdditionalOutput>
                    <Item>
                      <ForceProductSameAsFactoryOutput>1</ForceProductSameAsFactoryOutput>
                      <AdditionalOutputCycle>1</AdditionalOutputCycle>
                      <Amount>1</Amount>
                    </Item>
                  </AdditionalOutput>
                </ModOp>
              </Group>
          </Group>
        <!-- Tractors Barns have now a cycle from 180 sec-->
          <Group>
            <ModOp Type="merge" GUID='269837,269839,269848,269849,119026,119027' Path="/Values">
              <FactoryBase>
                <CycleTime>180</CycleTime>
              </FactoryBase>
            </ModOp>
            <ModOp Type="merge" GUID='1500300939' Path="/Values" Condition="@1500300939">
              <FactoryBase>
                <CycleTime>180</CycleTime>
              </FactoryBase>
            </ModOp>
          </Group>
        <!-- Modules have Maintenance -->
