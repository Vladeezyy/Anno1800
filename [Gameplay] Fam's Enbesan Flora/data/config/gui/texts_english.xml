	<!-- PURPOSE: Description for Calla Lily item -->
	<ModOp Path='/TextExport/Texts' Type="Add">
		<Text>
			<GUID>1660011311</GUID>
			<Text>&lt;b&gt;Zantedeschia aenbesica.&lt;/b&gt; Despite the 'lily' misnomer, this beautiful plant is temperamental yet majestic. Its elegant flowers are unmistakable.</Text>
		</Text>
	</ModOp>
	<!-- GROUP: Botanical set: Jewels of the South -->
	<!-- PURPOSE: Title for Royal Poinciana item -->
	<ModOp Path='/TextExport/Texts' Type="Add">
		<Text>
			<GUID>1660011312</GUID>
			<Text>Royal Poinciana</Text>
		</Text>
	</ModOp>

	<!-- GROUP: Botanical set: Jewels of the South -->
	<!-- PURPOSE: Description for Royal Poinciana item -->
	<ModOp Path='/TextExport/Texts' Type="Add">
		<Text>
			<GUID>1660011313</GUID>
			<Text>&lt;b&gt;Delonix regia.&lt;/b&gt; Commonly said to be the 'flamboyant tree', you can often see their bright red flowers lining the edges of roads.</Text>
		</Text>
	</ModOp>
	<!-- GROUP: Botanical set: Jewels of the South -->
	<!-- PURPOSE: Title for Spur Flower item -->
	<ModOp Path='/TextExport/Texts' Type="Add">
		<Text>
			<GUID>1660011314</GUID>
			<Text>Spur Flower</Text>
		</Text>
	</ModOp>

	<!-- GROUP: Botanical set: Jewels of the South -->
	<!-- PURPOSE: Description for Spur Flower item -->
	<ModOp Path='/TextExport/Texts' Type="Add">
		<Text>
			<GUID>1660011315</GUID>
			<Text>&lt;b&gt;Plectranthus coerulescens.&lt;/b&gt; This shrub blankets the land, bursting into flower late in the year with in pale flowers and fragrant leaves.</Text>
		</Text>
	</ModOp>
	<!-- GROUP: Botanical set: Jewels of the South -->
	<!-- PURPOSE: Title for Leopard Orchid item -->
	<ModOp Path='/TextExport/Texts' Type="Add">
		<Text>
			<GUID>1660011316</GUID>
			<Text>Leopard Orchid</Text>
		</Text>
	</ModOp>

	<!-- GROUP: Botanical set: Jewels of the South -->
	<!-- PURPOSE: Description for Leopard Orchid item -->
	<ModOp Path='/TextExport/Texts' Type="Add">
		<Text>
			<GUID>1660011317</GUID>
			<Text>&lt;b&gt;Ansellia africana.&lt;/b&gt; Sometimes the dark leopard spots may seem to take over the flower, but the striking resemblance to a leopard remains clear.</Text>
		</Text>
	</ModOp>
	<!-- GROUP: Botanical set: Jewels of the South -->
	<!-- PURPOSE: Title for Gazania item -->
	<ModOp Path='/TextExport/Texts' Type="Add">
		<Text>
			<GUID>1660011318</GUID>
			<Text>Gazania</Text>
		</Text>
	</ModOp>

	<!-- GROUP: Botanical set: Jewels of the South -->
	<!-- PURPOSE: Description for Gazania item -->
	<ModOp Path='/TextExport/Texts' Type="Add">
		<Text>
			<GUID>1660011319</GUID>
			<Text>&lt;b&gt;Gazania rigens.&lt;/b&gt; It's suggested that the petals taste like butter, leading to its other nickname 'the butter flower', although we don't recommend it on your toast.</Text>
		</Text>
	</ModOp>

	<!-- GROUP: Botanical set: Jewels of the South -->
